<div class="box-body">
    {{ watchViewer() }}
</div>
<div class="box-header with-border">
    <h3 class="box-title" data-toggle="tooltip" title="{{ entity.name }}">
        <img src="{{ entity.iconLink }}" class="img-thumbnail"> {{ entity.name|slice(0, 250) }}
    </h3>
</div>
<div class="box-footer clearfix">
    <div class="col-md-8">
        <div class="user-block">
            <img class="img-circle" src="https://almsaeedstudio.com/themes/AdminLTE/dist/img/user1-128x128.jpg" alt="User Image">
            <span class="username"><a href="#">{{ 'Jonathan Burke Jr.'|slice(0, 80) }}</a></span>
            <span class="description">Shared publicly - 7:30 PM Today</span>
        </div>
    </div>
    <div class="col-md-2 border-right">
        <div class="description-block">
            <h5 class="description-header">3,200</h5>
            <span class="description-text">compartido</span>
        </div>
    </div>
    <div class="col-md-2">
        <div class="description-block">
            <h5 class="description-header">13,000</h5>
            <span class="description-text">vistas</span>
        </div>
    </div>
</div>
<div class="box-footer clearfix">
    <div class="col-md-6" >
        <button type="button" class="btn btn-default btn-xs"><i class="fa fa-fw fa-tag"></i> Ingenieria</button>
        <button type="button" class="btn btn-default btn-xs"><i class="fa fa-fw fa-tag"></i> Tautologia</button>
        <button type="button" class="btn btn-default btn-xs"><i class="fa fa-fw fa-tag"></i> Gallinazo</button>
    </div>
    <div class="col-md-2 border-right">
        <div id="share-qr" class="description-block" >
            <button type="button" class="btn btn-default" data-toggle="tooltip" title="scanear desde movil">
                <i class="fa fa-qrcode"></i> Qr
            </button>
        </div>
    </div>
    <div class="col-md-2 border-right">
        <div class="description-block">
            <button type="button" class="btn btn-default" title="descargar archivo" data-toggle="modal" data-target="#modal-download">
                <i class="fa fa-download"></i> Descargar
            </button>
        </div>
    </div>
    <div class="col-md-2">
        <div class="description-block">
            <div class="btn-group">
                <button id="thumbs-down" type="button" class="btn btn-default">
                    <i class="fa fa-fw fa-thumbs-o-down"></i>
                </button>
                <button id="thumbs-up" type="button" class="btn btn-default">
                    <i class="fa fa-fw fa-thumbs-o-up"></i>
                </button>
            </div>
        </div>
    </div>
</div>