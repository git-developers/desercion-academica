{% for key, obj in accordion %}
    <div class="panel box box-primary">
        <div class="box-header with-border">
            <h4 class="box-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapse-{{ loop.index }}" aria-expanded="true" class="collapsed">
                    {{ obj.name }}
                </a>
            </h4>
        </div>
        <div id="collapse-{{ loop.index }}" class="panel-collapse collapse {{ loop.index == 1 ? 'in' : '' }}" aria-expanded="true">
            <div class="box-body">
                {{ include('BackendBundle:TemplateSetupEdit:Accordion/' ~ loop.index ~ '.html.twig') }}
            </div>
        </div>
    </div>
{% endfor %}